<h1>Create Item</h1>
<hr></hr>
<%= form_for @grocery do |f| %>
  <h2><%= label_tag(:title, "Title:") %></h2>
  <%= f.text_field :title %></br>
  <h2><%= label_tag(:description, "Description") %></h2>
  <%= f.text_area :description %></br>
  <br><%= f.submit "Create", :class => "btn btn-info" %>
<% end %></br>
<%=link_to "Grocery List", groceries_path, :class => "btn btn-warning" %>
